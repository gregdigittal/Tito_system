    <changeSet author="Pavlo.Slyvka" id="initiator_type-insert-init">
        <sql>
            INSERT INTO initiator_type (description, active, entity_id) VALUES ('cash', 1, null);
            INSERT INTO initiator_type (description, active, entity_id) VALUES ('icecash', 1, null);
            INSERT INTO initiator_type (description, active, entity_id) VALUES ('card', 1, (select id from entity where First_Name like '%CBZ Issuing Clearing Account%'));
            INSERT INTO initiator_type (description, active, entity_id) VALUES ('cardInternational', 1, null);
            INSERT INTO initiator_type (description, active, entity_id) VALUES ('ecocash', 1, (select id from entity where first_name = 'EcoCash TPI Account'));
            INSERT INTO initiator_type (description, active, entity_id) VALUES ('netone', 1, (select id from entity where first_name = 'NetOne' and last_name = 'ICEcash'));
        </sql>
    </changeSet>
